#if __LINUX__

; If some toolchain is missing then corresponding #define must be removed/commented.
#define HAVE_GCC
; .gcc_bin should be a full path to gcc binary.
; Can be obtained by running:  readlink -f $(which gcc)
.gcc_bin = '/usr/bin/x86_64-linux-gnu-gcc-6'
; .gcc_libexecdir should be a full path to the directory that contains internal compiler binaries (cc1 cc1plus).
; Can be obtained by running:  dirname $(readlink -f $(which $(gcc -print-prog-name=cc1)))
.gcc_libexecdir = '/usr/lib/gcc/x86_64-linux-gnu/6'
; Uncomment to enable checks for missing break in switch statements (GCC 7 or later is required):
; .gcc_ExtraCompilerOptions + ' -Werror=implicit-fallthrough'
; Uncomment if you want to build with Clang:
; #define HAVE_CLANG
; .clang_bin = '/usr/bin/x86_64-pc-linux-gnu-clang-3.8'

; .gcc_libexecdir should be a full path to the directory that contains the assebmler (as).
; Can be obtained by running:  dirname $(readlink -f $(which $(gcc -print-prog-name=as)))
.binutils_bindir = '/usr/bin'

; These lists can edited to remove unwanted configurations.
.Enabled_Platforms = { 'Linux32' 'Linux64' }
.Enabled_Configs = { 'Debug' 'Release' }
; Uncomment if you want to use sanitizers:
; .Enabled_Configs + { 'ASan' 'MSan' 'TSan' 'UBSan' }
.Enabled_Traits = { 'Default' }
